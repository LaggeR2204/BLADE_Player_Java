<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="60.0" prefWidth="800.0" style="-fx-background-color: rgb(35,35,35);-fx-border-radius: 5px;-fx-border-width: 1px;-fx-border-color: rgb(40,40,40);" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.PanelSongSearchDetailController">
   <children>
       <Label fx:id="lblSongName" layoutX="65.0" layoutY="10.0" prefHeight="20.0" text="Song Name" textFill="WHITE">
           <font>
               <Font name="Segoe UI Bold" size="13.0" />
           </font>
       </Label>
       <Label fx:id="lblArtist" layoutX="65.0" layoutY="30.0" prefHeight="20.0" text="Artist" textFill="WHITE">
           <font>
               <Font name="Arial Italic" size="13.0" />
           </font>
       </Label>
       <Label fx:id="lblSongNumber" alignment="CENTER_RIGHT" layoutX="605.0" layoutY="20.0" prefHeight="20.0" prefWidth="60.0" text="Number" textFill="WHITE">
           <font>
               <Font name="Arial Italic" size="13.0" />
           </font>
       </Label>
       <Label layoutX="670.0" layoutY="20.0" prefHeight="20.0" text="streams" textFill="WHITE">
           <font>
               <Font name="Arial Italic" size="13.0" />
           </font>
       </Label>
       <Button fx:id="btnDownload" alignment="TOP_LEFT" layoutX="745.0" layoutY="15.0" mnemonicParsing="false" onAction="#btnDownload_Clicked" prefHeight="30.0" prefWidth="30.0" style="-fx-background-color: transparent" textFill="#00000005">
           <cursor>
               <Cursor fx:constant="HAND" />
           </cursor>
         <graphic>
               <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                   <image>
                       <Image url="@../img/download_30px.png" />
                   </image>
               </ImageView>
         </graphic></Button>
      <ImageView fx:id="imgSongImage" fitHeight="50.0" fitWidth="50.0" layoutX="5.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/default_thumbnail_50px.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
